package com.bubblepipe;
import javax.swing.JFrame;

public class Game extends JFrame {
	
	public Game() {
		initUI();
	}
	
	protected void initUI() {
		add(new ChessBoard());
		
		setSize(250,200);
		
		setTitle("Bubble Pipe Chess");
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}

}
